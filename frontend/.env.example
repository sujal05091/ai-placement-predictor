# Google Gemini API
VITE_GEMINI_API_KEY=your_gemini_api_key

# Python Predictor API
VITE_PREDICTOR_API_URL=http://127.0.0.1:8080

# Power BI Dashboard
VITE_POWER_BI_EMBED_URL=https://app.powerbi.com/view?r=YOUR_REPORT_ID
